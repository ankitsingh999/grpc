#include<stdio.h>
#include<stdlib.h>
#include <string.h>
#include<conio.h>
//find-the-two-repeating-elements-in-a-given-array
void printRepeating(int arr[], int size)
{
int i,max=arr[0];
for(i=1; i<size;i++)
{
	if(arr[i]>max)
		max=arr[i];
}
//printf("\\max =%d\n",max);
int *count = (int *)calloc(sizeof(int), max+1);
printf(" Repeating elements with frequency are ");
printf(" \n\tElement\t frequency ");
int flag=0;
for(i = 0; i < size; i++)
{
	count[arr[i]]++;
}
for(i=0;i<=max;i++)
{
    if (count[i]>1)
    {
        printf("\n\t%d \t\t %d ", i, count[i]);
        flag++;
    }

}

if (flag==0)
	printf("\n\tNo duplicates");
}

int main()
{
int arr[100],n,i;
printf("Enter size ");
scanf("%d",&n) ;
for(i=0;i<n;i++)
	scanf("%d",&arr[i]);
printRepeating(arr, n);
getch();
return 0;
}
